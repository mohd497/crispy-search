.panel.panel-primary
  .panel-heading
    %h3.panel-title Upload CSV

  .panel-body
    .row
      .col-sm-12
        .text-center
          = form_tag upload_csv_path, multipart: true, class: 'form-inline' do
            .form-group
              = file_field_tag :csv_file, accept: '.csv'
            = submit_tag 'Upload', class: 'btn btn-success'
